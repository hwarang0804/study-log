   "source": [
    "dials = ['ABC', 'DEF', 'GHI', 'JKL', 'MNO', 'PQRS', 'TUV', 'WXYZ']\n",
    "word = input()\n",
    "result = 0\n",
    "\n",
    "for char in word:\n",
    "    for i, dial in enumerate(dials):\n",
    "        if char in dial:\n",
    "            result += i + 3\n",
    "            break\n",
    "\n",
    "print(result)"
   ]
  }
