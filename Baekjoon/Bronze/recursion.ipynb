    "def draw(h):\n",
    "    if h == 0:\n",
    "        return\n",
    "    \n",
    "    draw(h-1)\n",
    "\n",
    "    for i in range(1, h + 1):\n",
    "        print(\"*\", end='')\n",
    "    print()\n",
    "    \n",
    "\n",
    "Height = int(input())\n",
    "\n",
    "draw(Height)\n",
    "\n",
    "\n"
