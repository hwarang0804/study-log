   "source": [
    "N, K = map(int, input().split())\n",
    "\n",
    "factors = []\n",
    "\n",
    "for i in range(N):\n",
    "    if N % (i + 1) == 0:\n",
    "        factors.append(i + 1)\n",
    "\n",
    "try:\n",
    "    print(factors[K-1])\n",
    "except:\n",
    "    print(0)\n"
   ]
