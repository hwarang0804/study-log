   "source": [
    "list = [8 ,7, 4, 5, 6, 3, 2, 1]\n",
    "n = len(list)\n",
    "i = 0\n",
    "\n",
    "\n",
    "while i < n - 1:\n",
    "    j = 0\n",
    "    while j < n - i - 1:\n",
    "        if list[j] > list[j + 1]:\n",
    "            list[j], list[j + 1] = list[j + 1], list[j]         \n",
    "        j += 1\n",
    "    i += 1\n",
    "\n",
    "list"
   ]
