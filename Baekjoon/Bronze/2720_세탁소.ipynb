    "\n",
    "for i in range(N):\n",
    "    price = int(input())\n",
    "    \n",
    "    one = price // 25 \n",
    "    two = (price % 25) // 10\n",
    "    three = (price % 25 % 10) // 5\n",
    "    four = price % 25 % 10 % 5\n",
    "     \n",
    "    print(one, end=\" \")\n",
    "    print(two, end=\" \")\n",
    "    print(three, end=\" \")\n",
    "    print(four)\n",
