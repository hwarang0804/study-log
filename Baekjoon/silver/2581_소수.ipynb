   "source": [
    "M = int(input())\n",
    "N = int(input())\n",
    "\n",
    "results = []\n",
    "\n",
    "for i in range(M , N + 1):\n",
    "    for j in range(i):\n",
    "        if i % (j + 2) == 0:\n",
    "            break\n",
    "    if i == j + 2:\n",
    "        results.append(i)\n",
    "\n",
    "if not results:\n",
    "    print(-1)\n",
    "else:\n",
    "    print(sum(results))\n",
    "    print(results[0])\n",
    "\n",
    "\n"
   ]
